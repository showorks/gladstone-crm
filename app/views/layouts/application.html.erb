      </div>
    <% end %>
    <% if alert %>
      <div class="alert alert-danger fade in" data-alert="alert">
        <a class="close" data-dismiss='alert' href="#">×</a>
        <p><%= alert %></p>
      </div>
    <% end %>

    <% if jobs_running? %>
      <div class="alert alert-danger fade in" data-alert="alert">
        <a class="close" data-dismiss='alert' href="#">×</a>
        <p>The CRM is currently offline while the database is being updated.</p>
      </div>
    <% end %>

    <%= yield unless jobs_running? %>
  </div>

  <footer class="footer">
    <div class="container">
      <center><br><a href="http://www.fairsoftware.com">Gladstone Inc.  &copy; <%= Date.today.year %> &nbsp;</a> <br><br></center>
    </div>
  </footer>

</body>
</html>
